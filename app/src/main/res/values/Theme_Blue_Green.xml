<?xml version="1.0" encoding="utf-8"?>
<resources>

    <color name="primarybg">#2196F3</color>
    <color name="primarybgfb100">#BBDEFB</color>
    <color name="primarybgfb300">#64B5F6</color>
    <color name="primary_dark_bg">#1976D2</color>
    <color name="primary_light_bg">#BBDEFB</color>
    <color name="accentbg">#4CAF50</color>
    <color name="accentbgfb100">#c8e6c9</color>
    <color name="accentbgfb300">#81c784</color>
    <color name="primary_text_bg">#212121</color>
    <color name="secondary_text_bg">#727272</color>
    <color name="text_on_primary_bg">#ffffff</color>
    <color name="iconTintNormalbg">#616161</color>
    <color name="dividerbg">#B6B6B6</color>

    <style name="BasicTheme.BlueGreen" parent="AppTheme.BlueGreen">
        <item name="cardBackground">@color/primarybgfb100</item>
        <item name="android:windowBackground">@color/primarybgfb300</item>
        <item name="iconStyle">@style/iconStyle.BlueGreen</item>
    </style>

    <style name="AppTheme.BlueGreen" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primarybg</item>
        <item name="colorPrimaryFallbackMed">@color/primarybgfb300</item>
        <item name="colorPrimaryFallbackLow">@color/primarybgfb100</item>
        <item name="colorAccentFallbackMed">@color/accentbgfb300</item>
        <item name="colorAccentFallbackLow">@color/accentbgfb100</item>
        <item name="colorPrimaryDark">@color/primary_dark_bg</item>
        <item name="colorAccent">@color/accentbg</item>
        <item name="customToolBarStyle">@style/ToolBarStyle.BlueGreen</item>
        <!--<item name="textColorPrimary">@color/colorTextPrimaryLT</item> -->
    </style>

    <style name="ToolBarStyle.BlueGreen" parent="Widget.Design.TabLayout">
        <item name="tabIndicatorColor">@color/color_white</item>
        <item name="tabIndicatorHeight">@dimen/tab_indicator_height</item>
        <item name="tabTextAppearance">@style/TabLayoutTextStyle</item>
        <item name="tabBackground">@color/primarybg</item>
    </style>

    <style name="iconStyle.BlueGreen" parent="iconStyle">
        <item name="android:tint">@color/iconTintNormalbg</item>
        <item name="tintHighlight">@color/accentbg</item>
    </style>

</resources>